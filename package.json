{
	"name": "flutter-clean-architecture",
	"displayName": "Flutter Clean Architecture",
	"publisher": "korich",
	"repository": "https://github.com/korich/vscode-flutter-clean-architecture",
	"description": "",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:korich-fca.createFeature",
		"onCommand:korich-fca.setUp"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "korich-fca.createFeature",
				"title": "Flutter: Create Feature"
			},
			{
				"command": "korich-fca.setUp",
				"title": "Flutter: Set Up Clean Architecture Project"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.45.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
